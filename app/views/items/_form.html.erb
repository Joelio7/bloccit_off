<h1>item creation</h1>
<div class="col-md-8">
  <%= form_for [@user, item] do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control', placeholder: "Create item" %>
    </div>
    <%= f.submit "Save", class: 'btn btn-success' %>

    <% end %>
  </div>
